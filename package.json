{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "jszip-cli": "dist/cli.js"
  },
  "dependencies": {
    "commander": "3.0.0",
    "cosmiconfig": "5.2.1",
    "fs-extra": "8.1.0",
    "jszip": "3.2.2",
    "logdown": "3.2.8",
    "progress": "2.0.3"
  },
  "description": "A zip CLI based on jszip.",
  "devDependencies": {
    "@ffflorian/prettier-config": "0.0.5",
    "@ffflorian/semantic-release-config": "0.1.2",
    "@ffflorian/tslint-config": "0.5.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/npm": "5.1.15",
    "@types/cosmiconfig": "5.0.3",
    "@types/fs-extra": "8.0.0",
    "@types/jasmine": "3.4.0",
    "@types/jszip": "3.1.6",
    "@types/node": "~12",
    "@types/progress": "2.0.3",
    "husky": "3.0.4",
    "jasmine": "3.4.0",
    "lint-staged": "9.2.4",
    "prettier": "1.18.2",
    "publish-flat": "1.3.1",
    "rimraf": "3.0.0",
    "semantic-release": "15.13.24",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "jszip",
    "typescript",
    "zip"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "spec/**/*.ts": [
      "tslint --project tsconfig.jasmine.json --fix",
      "git add"
    ],
    "src/**/*.ts": [
      "tslint --project tsconfig.json --fix",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/jszip-cli",
  "prettier": "@ffflorian/prettier-config",
  "release": {
    "extends": "@ffflorian/semantic-release-config"
  },
  "repository": "https://github.com/ffflorian/jszip-cli.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "flatten": "publish-flat -o flattened",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix && yarn lint:ts:spec --fix",
    "lint": "yarn lint:ts && yarn lint:ts:spec && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:spec": "tslint --project tsconfig.jasmine.json",
    "prettier": "prettier \"*.{json,md}\"",
    "release": "semantic-release",
    "start": "ts-node src/cli.ts",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/.bin/jasmine"
  },
  "version": "2.2.0"
}
