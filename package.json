{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "jszip-cli": "dist/cli.js"
  },
  "dependencies": {
    "commander": "2.19.0",
    "cosmiconfig": "5.0.7",
    "fs-extra": "7.0.1",
    "jszip": "3.1.5",
    "logdown": "3.2.7",
    "progress": "2.0.3"
  },
  "description": "A zip CLI based on jszip.",
  "devDependencies": {
    "@types/cosmiconfig": "5.0.3",
    "@types/fs-extra": "5.0.5",
    "@types/jasmine": "3.3.8",
    "@types/jszip": "3.1.4",
    "@types/node": "11.9.0",
    "@types/progress": "2.0.3",
    "husky": "1.3.1",
    "jasmine": "3.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "cli",
    "jszip",
    "typescript",
    "zip"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/jszip-cli",
  "repository": "https://github.com/ffflorian/jszip-cli.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.ts\"",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/jasmine/bin/jasmine"
  },
  "types": "dist/index.d.ts",
  "version": "2.1.4"
}
